    "zod": "^3.22.4"
  },
  "devDependencies": {
    "prisma": "^5.8.0",
    "ts-node": "^10.9.2",
    "tsup": "^8.0.1",
    "tsx": "^4.7.1",
    "typescript": "5.2.2",
    "zod-prisma": "^0.5.4"
  }
